    "axios": "^1.3.4",
    "better-sqlite3": "^8.7.0",
    "electron-updater": "^4.6.5",
    "eslint": "^8.30.0",
    "eslint-config-react-app": "^7.0.1",
    "fast-glob": "^3.2.12",
    "music-metadata": "^7.13.0",
    "react-data-grid": "^7.0.0-beta.34",
    "react-icons": "^4.7.1",
